{
  "name": "ion4-calendar",
  "version": "3.0.4-beta.6",
  "description": "A date picker component for ionic4",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "cd ./dev && ionic serve",
    "clean:dist": "rm -rf dist",
    "demo:link": "npm link && cd ./demo && npm link ion2-calendar",
    "demo:serve": "cd ./demo && ionic serve",
    "build": "run-s clean:dist build:copy-sources build:ts && gulp copy-scss ",
    "build:ts": "tsc && ngc",
    "build:copy-sources": "gulp copy-sources",
    "test": "echo \"Error: no test specified\"",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "packagr": "ng-packagr -p package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rift-walker/ion2-calendar.git"
  },
  "devDependencies": {
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/compiler-cli": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.0.0",
    "@ionic/angular": "4.0.0-alpha.11",
    "ionicons": "~3.0.0",
    "ng-packagr": "^4.0.0",
    "npm-run-all": "^4.1.2",
    "rxjs": "6.2.1",
    "tsickle": "^0.27.2",
    "tslint": "~5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.5.3",
    "zone.js": "0.8.26"
  },
  "keywords": [
    "ionic4",
    "calendar",
    "Datepicker",
    "date-range"
  ],
  "author": "HsuanLee",
  "license": "MIT",
  "peerDependencies": {
    "moment": "^2.22.2"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts",
      "styleIncludePaths": [
        ""
      ],
      "umdModuleIds": {
        "moment": "moment"
      }
    }
  }
}
